{
    "name": "sethobey/laravel-taxjar",
    "description": "Friendly wrapper of TaxJar's REST API SDK with a few extras for Laravel 6.0 and up",
    "keywords": [
      "laravel",
      "taxjar",
      "tax",
      "estimate",
      "api",
      "logging",
      "logs"
    ],
    "type": "library",
    "homepage": "https://github.com/sethobey/laravel-taxjar",
    "license": "MIT",
    "authors": [
        {
            "name": "Seth Obey",
            "email": "sethobey@gmail.com",
            "homepage": "https://github.com/sethobey",
            "role": "Developer"
        }
    ],
    "require": {
      "php" : "^7.2.5|^8.0",
      "illuminate/container": "^6.0|^7.0|^8.0",
      "illuminate/contracts": "^6.0|^7.0|^8.0",
      "taxjar/taxjar-php": "^1.10"
    },
    "require-dev": {
      "orchestra/testbench": "^4.0|^5.0|^6.0",
      "phpunit/phpunit": "^8.0|^9.0",
        "laravel/tinker": "^2.6"
    },
    "autoload": {
      "psr-4": {
          "LaraJar\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraJar\\Test\\": "tests"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "LaraJar\\TaxJarServiceProvider"
          ]
      }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
